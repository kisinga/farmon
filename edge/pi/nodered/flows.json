    {
        "id": "dash1",
        "type": "function",
        "z": "dashboard-tab",
        "name": "Extract Battery",
        "func": "// Extract battery percentage from sensor data\nconst data = msg.sensorData || {};\nconst batteryPercent = data.bp; // Battery percent key is 'bp'\n\n// Only send to dashboard if battery data exists\nif (batteryPercent !== undefined && batteryPercent !== null) {\n    msg.payload = parseFloat(batteryPercent);\n    msg.topic = msg.deviceName || msg.deviceEui || 'device';\n    msg.deviceInfo = {\n        name: msg.deviceName,\n        eui: msg.deviceEui\n    };\n    return msg;\n}\nreturn null;",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 340,
        "y": 100,
        "wires": [["dash-gauge", "dash-battery-text"]]
