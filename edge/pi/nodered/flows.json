    {
        "id": "n5a2b3c4d5e6f7g8",
        "type": "postgresql",
        "z": "farm-flow-tab",
        "name": "Store Reading",
        "query": "INSERT INTO readings (device_eui, data) VALUES ($1, $2::jsonb) RETURNING id",
        "postgreSQLConfig": "a1b2c3d4e5f6a7b9",
        "split": false,
        "rowsPerMsg": 1,
        "outputs": 1,
        "x": 530,
